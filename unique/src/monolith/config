

monolith.state = enable
monolith.design = option

monolith.dep = $(T.target)

m.suf=);
m.pre=\nUNMODULE(

$(call C.S,monolith,main): $(call C.H,monolith,modules)

$(call C.H,monolith,modules): 
	@$(call U.RM,$@)
	@$(call U.EF,/*\n * Automatic generated modules define header\n */,$@)
	@$(call U.EF,$(addsuffix $(m.suf),$(addprefix $(m.pre),$(monolith.dep))),$@)

.PHONY: $(call C.H,monolith,modules)
