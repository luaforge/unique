

runable.state = enable
runable.design = option

runable.dep = $(T.target)

m.suf=);
m.pre=\nUNMODULE(

$(call C.S,runable,main): $(call C.H,runable,modules)

$(call C.H,runable,modules): 
	@$(call U.EX,$(call U.RM,$@))
	@$(call U.EX,$(call U.EF,/*,$@))
	@$(call U.EX,$(call U.EF, * Automatic generated modules define header,$@))
	@$(call U.EX,$(call U.EF, */,$@))
	@$(call U.EX,$(call U.EF,$(addsuffix $(m.suf),$(addprefix $(m.pre),$(runable.dep))),$@))

.PHONY: $(call C.H,runable,modules)
